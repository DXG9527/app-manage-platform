{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        [
            "import",
            [
                {
                    "libraryName": "antd",
                    "style": true
                },
                {
                    "libraryName": "amp",
                    "libraryDirectory": "lib",
                    "style": true
                }
            ]
        ],
        "transform-decorators-legacy"
    ],
    "env": {
        "build": {
            "optional": [
                "optimisation",
                "minification"
            ]
        }
    }
}
